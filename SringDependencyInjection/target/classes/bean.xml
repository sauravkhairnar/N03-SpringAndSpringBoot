<?xml version="1.0" encoding="UTF-8"?>
<!--Context Schema -->
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context" xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd"> <!-- bean definitions here -->
<bean id="swift" class="org.tnsif.SringDependencyInjection.SwiftEngine">
<!--property name="company" value="Maruti Suzuki"/>
<property name="cost" value="500000"/-->

<constructor-arg name ="company" value =" TATA"></constructor-arg>
<constructor-arg name ="cost" value =" 1200000"></constructor-arg>

</bean>


<bean id="Renault REX" class="org.tnsif.SringDependencyInjection.SwiftEngine" >
<!--property name="company" value="Renault REX"/>
<property name="cost" value="500000"/-->
<constructor-arg name ="company" value =" Renault"></constructor-arg>
<constructor-arg name ="cost" value =" 1200000"></constructor-arg>
</bean>


</beans>